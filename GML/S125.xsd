						<xs:annotation>
							<xs:documentation>A statement expressing if a light is considered to be a major light in terms of ECDIS display in a particular area.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lightVisibility" type="lightVisibilityType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The specific visibility of a light, with respect to the light&#39;s intensity and ease of recognition.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="exhibitionConditionOfLight" type="exhibitionConditionOfLightType" minOccurs="0" maxOccurs="1">
