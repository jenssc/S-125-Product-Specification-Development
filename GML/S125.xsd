	<xs:complexType name="horizontalPositionUncertaintyType">
		<xs:annotation>
			<xs:documentation>The best estimate of the accuracy of a position.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="uncertaintyFixed" type="xs:double" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The best estimate of the fixed horizontal or vertical accuracy component for positions, depths, heights, vertical distances and vertical clearances.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="uncertaintyVariableFactor" type="xs:double" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The factor to be applied to the variable component of an uncertainty equation so as to provide the best estimate of the variable horizontal or vertical accuracy component for positions, depths, heights, vertical distances and vertical clearances.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="verticalUncertaintyType">
		<xs:annotation>
			<xs:documentation>The best estimate of the vertical accuracy of depths, heights, vertical distances and vertical clearances.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="uncertaintyFixed" type="xs:double" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The best estimate of the fixed horizontal or vertical accuracy component for positions, depths, heights, vertical distances and vertical clearances.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="uncertaintyVariableFactor" type="xs:double" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The factor to be applied to the variable component of an uncertainty equation so as to provide the best estimate of the variable horizontal or vertical accuracy component for positions, depths, heights, vertical distances and vertical clearances.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="directionalCharacterType">
		<xs:annotation>
			<xs:documentation>A directional light is a light illuminating a sector of very narrow angle and intended to mark a direction to follow.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="moireEffect" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>A short range (up to 2km) type of directional light. Sodium lighting gives a yellow background to a screen on which a vertical black line will be seen by an observer on the centre line.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orientation" type="orientationType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>(1) The angular distance measured from true north to the major axis of the feature. (2) In ECDIS, the mode in which information on the ECDIS is being presented. Typical modes include: north-up - as shown on a nautical chart, north is at the top of the display; Ships head-up - based on the actual heading of the ship, (e.g. Ships gyrocompass); course-up display - based on the course or route being taken.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="featureNameType">
		<xs:annotation>
			<xs:documentation>Provides the name of an entity, defines the national language of the name, and provides the option to display the name at various system display settings.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="language" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The method of human communication, either spoken or written, consisting of the use of words in a structured and conventional way.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The individual name of a feature.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nameUsage" type="nameUsageType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Classification of the type and display level of the name of a feature in an end-user system.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="fixedDateRangeType">
		<xs:annotation>
			<xs:documentation>An active period of a single fixed event or occurrence, as the date range between discrete start and end dates.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dateEnd" type="S100_TruncatedDate" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The latest date on which an object (for example a buoy) will be present.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dateStart" type="S100_TruncatedDate" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The earliest date on which an object (for example a buoy) will be present.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="timeOfDayEnd" type="xs:time" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The time corresponding to the end of an active period.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="timeOfDayStart" type="xs:time" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The time corresponding to the start of an active period.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="informationType">
		<xs:annotation>
			<xs:documentation>Textual information about the feature. The information may be provided as a string of text or as a file name of a single external text file that contains the text.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="fileLocator" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The location of a fragment of text or other information in a support file.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fileReference" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The file name of an externally referenced text file.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="headline" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Words set at the head of a passage or page to introduce or categorize.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="language" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The method of human communication, either spoken or written, consisting of the use of words in a structured and conventional way.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="text" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>A non-formatted digital text string.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="lightSectorType">
		<xs:annotation>
			<xs:documentation>A sector is the part of a circle between two straight lines drawn from the centre to the circumference.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="colour" type="colourType" minOccurs="1" maxOccurs="99">
				<xs:annotation>
					<xs:documentation>The property possessed by an object of producing different sensations on the eye as a result of the way it reflects or emits light.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="directionalCharacter" type="directionalCharacterType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>A directional light is a light illuminating a sector of very narrow angle and intended to mark a direction to follow.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lightVisibility" type="lightVisibilityType" minOccurs="0" maxOccurs="99">
				<xs:annotation>
					<xs:documentation>The specific visibility of a light, with respect to the light&#39;s intensity and ease of recognition.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sectorLimit" type="sectorLimitType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>A sector is the part of a circle between two straight lines drawn from the centre to the circumference. The sector limit specifies the limits of the sector In a clockwise direction around the central feature (for example a light).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="valueOfNominalRange" type="xs:double" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The luminous range of a light in a homogenous atmosphere in which the meteorological visibility is 10 sea miles.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sectorInformation" type="sectorInformationType" minOccurs="0" maxOccurs="99">
				<xs:annotation>
					<xs:documentation>Additional textual information about a light sector.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sectorArcExtension" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>An indication that the default radius of a sector arc is to be extended by 5mm.</xs:documentation>
				</xs:annotation>
