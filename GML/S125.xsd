							<xs:documentation>The mechanism used to generate a fog or light signal.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="marksNavigationalSystemOf" type="marksNavigationalSystemOfType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The system of navigational buoyage a region complies with.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="exhibitionConditionOfLight" type="exhibitionConditionOfLightType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The outward display of the light.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="categoryOfLight" type="categoryOfLightType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Classification of different light types.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="sectorCharacteristics" type="sectorCharacteristicsType" minOccurs="1" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Describes the characteristics of a light sector.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="multiplicityOfFeatures" type="multiplicityOfFeaturesType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The number of features of identical character that exist as a co-located group.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="geometry" maxOccurs="unbounded">
