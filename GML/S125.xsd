			<xs:extension base="S100:AbstractFeatureType">
				<xs:sequence>
					<xs:element name="categoryOfLandmark" type="categoryOfLandmarkType" minOccurs="1" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Classification of prominent cultural and natural features in the landscape.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="colour" type="colourType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>The property possessed by an object of producing different sensations on the eye as a result of the way it reflects or emits light.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="colourPattern" type="colourPatternType" minOccurs="0" maxOccurs="unbounded">
